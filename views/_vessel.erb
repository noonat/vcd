<div class="vessel">
   <%= tty_h2 "ARCHIVE DATA FILE <a href=\"#{vessel.href}\">#{vessel.id}.VCD</a>" %>
   <div class="stats">
      <div class="logged">logged by pilot <%= vessel.md5 %> on <%= eighties_time(vessel.created_at)[0,10] %></div>
      <div class="piloted"><%=
         unless @vessel_pilot_clicks.nil?
            @vessel_pilot_clicks[vessel.id]
         else
            vessel.vessel_pilot_clicks.length + 1
         end
      %> known pilot(s)</div>
      <div style="clear:both;"></div>
   </div>
   <br/>
   <div class="data"><%= vessel.data_trimmed %></div>
   <div class="link">[<a href="<%= vessel.pilot_href(true) %>">PILOT THIS VESSEL</a>]</div>
</div>
